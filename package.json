{
  "name": "frida-sidecar",
  "version": "0.11.2",
  "description": "Sidecar is for easy hook/call in-process functions with TypeScript annotations.",
  "main": "dist/src/mod.js",
  "typings": "dist/src/mod.d.ts",
  "engines": {
    "node": ">=10"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "bin": {
    "sidecar-dump": "dist/bin/sidecar-dump.js"
  },
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc && shx cp -R src/agent/templates/{libs,*.mustache} dist/src/agent/templates/",
    "lint": "npm run lint:es && npm run lint:ts",
    "lint:ts": "tsc --noEmit",
    "pack": "npm pack",
    "start": "npm run example",
    "example": "ts-node examples/main.ts",
    "chatbox": "cd examples/chatbox && make",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"tests/**/*.spec.ts\"",
    "test:unit:debug": "cross-env BROLOG_LEVEL=silly blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"tests/**/*.spec.ts\"",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "lint:es": "eslint --ignore-pattern fixtures/ \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huan/sidecar.git"
  },
  "keywords": [
    "frida",
    "hook"
  ],
  "author": "Huan <zixia@zixia.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/huan/sidecar/issues"
  },
  "homepage": "https://github.com/huan/sidecar#readme",
  "devDependencies": {
    "@chatie/eslint-config": "^0.12.4",
    "@chatie/git-scripts": "^0.6.2",
    "@chatie/semver": "^0.4.7",
    "@chatie/tsconfig": "^0.16.2",
    "@types/mustache": "^4.1.1",
    "@types/node": "^16.0.0",
    "@types/string-similarity": "^4.0.0",
    "cross-env": "^7.0.3",
    "frida-compile": "^10.2.4",
    "pkg-jq": "^0.2.11",
    "shx": "^0.3.3",
    "string-similarity": "^4.0.4",
    "tsd": "^0.17.0",
    "tstest": "^0.4.10",
    "typescript": "=4.4.0-dev.20210706"
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  },
  "publishConfig": {
    "tag": "next"
  },
  "dependencies": {
    "@types/frida-gum": "^17.0.0",
    "brolog": "^1.12.4",
    "cmd-ts": "^0.7.0",
    "frida": "^15.0.12",
    "mustache": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "slash": "^3.0.0",
    "typed-emitter": "^1.3.1"
  }
}
